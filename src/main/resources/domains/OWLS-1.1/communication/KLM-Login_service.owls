<service:Service rdf:ID="LINGUISTICEXPRESSION-LINGUISTICEXPRESSION_SERVICE">
<service:presents rdf:resource="#LINGUISTICEXPRESSION-LINGUISTICEXPRESSION_PROFILE"/>
