<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/ontosem.owl"/>
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/core-plus-office.owl" />
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/ShoppingCart.owl" />
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/Expression.owl" />
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/PDDLExpression.owl" />
</owl:Ontology>

<service:Service rdf:ID="CREDITACCOUNTBOOKPERSON__SERVICE">
<service:presents rdf:resource="#CREDITACCOUNTBOOKPERSON__PROFILE"/>
<service:describedBy rdf:resource="#CREDITACCOUNTBOOKPERSON__PROCESS"/>
<service:supports rdf:resource="#CREDITACCOUNTBOOKPERSON__GROUNDING"/>
</service:Service>

<profile:Profile rdf:ID="CREDITACCOUNTBOOKPERSON__PROFILE">
<service:isPresentedBy rdf:resource="#CREDITACCOUNTBOOKPERSON__SERVICE"/>
<profile:serviceName xml:lang="en">
Bea Book Shopping
</profile:serviceName>
<profile:textDescription xml:lang="en">
This service checks the validity of a credit account of an authorised person and adds the selected book in his shopping cart.
</profile:textDescription>
<profile:hasInput  rdf:resource="#_PERSON"/>
<profile:hasInput  rdf:resource="#_BOOK"/>
<profile:hasInput  rdf:resource="#_CREDITACCOUNT"/>

<profile:has_process rdf:resource="CREDITACCOUNTBOOKPERSON__PROCESS" /></profile:Profile>

<!--<process:ProcessModel rdf:ID="CREDITACCOUNTBOOKPERSON__PROCESS_MODEL">
<service:describes rdf:resource="#CREDITACCOUNTBOOKPERSON__SERVICE"/>
<process:hasProcess rdf:resource="#CREDITACCOUNTBOOKPERSON__PROCESS"/>
</process:ProcessModel>-->

<process:AtomicProcess rdf:ID="CREDITACCOUNTBOOKPERSON__PROCESS">
<service:describes rdf:resource="#CREDITACCOUNTBOOKPERSON__SERVICE"/>
<process:hasInput  rdf:resource="#_PERSON"/>
<process:hasInput  rdf:resource="#_BOOK"/>
<process:hasInput  rdf:resource="#_CREDITACCOUNT"/>
<process:hasPrecondition>
          <pddlexpr:PDDL-Expression>
            <expr:expressionBody rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
			(and (http://127.0.0.1:8000/ontology/ontosem.owl#Accepted ?http://127.0.0.1:8000/services/OWLS-1.1/bookpersoncreditaccount__Beaservice.owls#_CREDITACCOUNT)
			      (http://127.0.0.1:8000/ontology/core-plus-office.owl#Authorized ?http://127.0.0.1:8000/services/OWLS-1.1/bookpersoncreditaccount__Beaservice.owls#_PERSON)
				  (http://127.0.0.1:8000/ontology/Mid-level-ontology.owl#accountHolder ?http://127.0.0.1:8000/services/OWLS-1.1/bookpersoncreditaccount__Beaservice.owls#_PERSON ?http://127.0.0.1:8000/services/OWLS-1.1/bookpersoncreditaccount__Beaservice.owls#_CREDITACCOUNT))
			</expr:expressionBody>
          </pddlexpr:PDDL-Expression>
</process:hasPrecondition>
<process:hasResult>
          <process:Result>
            <process:hasEffect>
              <pddlexpr:PDDL-Expression>
                <expr:expressionBody rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
				(http://127.0.0.1:8000/ontology/ShoppingCart.owl#ShoppingCartRequestItems ?http://127.0.0.1:8000/services/OWLS-1.1/bookpersoncreditaccount__Beaservice.owls#_BOOK)
				</expr:expressionBody>
              </pddlexpr:PDDL-Expression>
            </process:hasEffect>
          </process:Result>
</process:hasResult>
</process:AtomicProcess>

<process:Input rdf:ID="_PERSON">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1:8000/ontology/books.owl#Person</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Input rdf:ID="_BOOK">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1:8000/ontology/books.owl#Book</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Input rdf:ID="_CREDITACCOUNT">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1:8000/ontology/Mid-level-ontology.owl#CreditAccount</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

