<?xml version="1.0" encoding="WINDOWS-1252"?>
<rdf:RDF  xmlns:owl       = "http://www.w3.org/2002/07/owl#"
xmlns:rdfs      = "http://www.w3.org/2000/01/rdf-schema#"
xmlns:rdf       = "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
xmlns:service   = "http://www.daml.org/services/owl-s/1.1/Service.owl#"
xmlns:process   = "http://www.daml.org/services/owl-s/1.1/Process.owl#"
xmlns:profile    = "http://www.daml.org/services/owl-s/1.1/Profile.owl#"
xmlns:grounding = "http://www.daml.org/services/owl-s/1.1/Grounding.owl#"
xmlns:expr      = "http://www.daml.org/services/owl-s/1.1/generic/Expression.owl#"
xmlns:pddlexpr  = "http://127.0.0.1:8000/ontology/PDDLExpression.owl#"

xml:base        = "http://127.0.0.1:8000/services/OWLS-1.1/city_hotel_Saarlandservice.owls">

<owl:Ontology rdf:about="">
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/Service.owl" />
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/Process.owl" />
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/Profile.owl" />
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/Grounding.owl" />
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/Expression.owl" />
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/PDDLExpression.owl" />
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/travel.owl" />
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/protont.owl" />
<owl:imports rdf:resource="http://127.0.0.1:8000/ontology/geographydataset.owl" />

</owl:Ontology>

<service:Service rdf:ID="CITY_HOTEL_SERVICE">
<service:presents rdf:resource="#CITY_HOTEL_PROFILE"/>
<service:describedBy rdf:resource="#CITY_HOTEL_PROCESS"/>
<service:supports rdf:resource="#CITY_HOTEL_GROUNDING"/>
</service:Service>

<profile:Profile rdf:ID="CITY_HOTEL_PROFILE">
<service:isPresentedBy rdf:resource="#CITY_HOTEL_SERVICE"/>
<profile:serviceName xml:lang="en">
CityHotelInfoService
</profile:serviceName>
<profile:textDescription xml:lang="en">
This service returns available hotels in a given city from Saarland state in germany.
</profile:textDescription>
<profile:hasInput  rdf:resource="#_CITY"/>
<profile:hasOutput rdf:resource="#_HOTEL"/>
<profile:hasLocal rdf:resource="#_SAARLAND"/>

<profile:has_process rdf:resource="CITY_HOTEL_PROCESS" /></profile:Profile>

<!--<process:ProcessModel rdf:ID="CITY_HOTEL_PROCESS_MODEL">
<service:describes rdf:resource="#CITY_HOTEL_SERVICE"/>
<process:hasProcess rdf:resource="#CITY_HOTEL_PROCESS"/>
</process:ProcessModel>-->

<process:AtomicProcess rdf:ID="CITY_HOTEL_PROCESS">
<service:describes rdf:resource="#CITY_HOTEL_SERVICE"/>
<process:hasInput  rdf:resource="#_CITY"/>
<process:hasOutput rdf:resource="#_HOTEL"/>
<process:hasLocal rdf:resource="#_SAARLAND"/>
<process:hasPrecondition>
          <pddlexpr:PDDL-Expression>
            <expr:expressionBody rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
				(http://127.0.0.1:8000/ontology/protont.owl#locatedIn ?http://127.0.0.1:8000/services/OWLS-1.1/city_hotel_Saarlandservice.owls#_CITY ?http://127.0.0.1:8000/services/OWLS-1.1/city_hotel_Saarlandservice.owls#_SAARLAND)
			</expr:expressionBody>
          </pddlexpr:PDDL-Expression>
</process:hasPrecondition>
</process:AtomicProcess>

<process:Input rdf:ID="_CITY">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1:8000/ontology/travel.owl#City</process:parameterType>
<rdfs:label></rdfs:label>
</process:Input>

<process:Output  rdf:ID="_HOTEL">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1:8000/ontology/travel.owl#Hotel</process:parameterType>
<rdfs:label></rdfs:label>
</process:Output>

<process:Local  rdf:ID="_SAARLAND">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1:8000/ontology/geographydataset.owl#Saarland</process:parameterType>
<rdfs:label></rdfs:label>
</process:Local>

<grounding:WsdlGrounding rdf:ID="CITY_HOTEL_GROUNDING">
<service:supportedBy rdf:resource="#CITY_HOTEL_SERVICE"/>
<grounding:hasAtomicProcessGrounding>
      <grounding:WsdlAtomicProcessGrounding rdf:ID="CITY_HOTEL_AtomicProcessGrounding"/>
    </grounding:hasAtomicProcessGrounding>
</grounding:WsdlGrounding>

<grounding:WsdlAtomicProcessGrounding rdf:about="#CITY_HOTEL_AtomicProcessGrounding">
    <grounding:wsdlDocument rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1:8000/wsdl/CityHotel.wsdl</grounding:wsdlDocument>
    <grounding:owlsProcess rdf:resource="#CITY_HOTEL_PROCESS"/>
    <grounding:wsdlOperation>
      <grounding:WsdlOperationRef>
        <grounding:operation rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1:8000/wsdl/CityHotel#get_HOTEL</grounding:operation>
        <grounding:portType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1:8000/wsdl/CityHotel#CityHotelSoap</grounding:portType>
      </grounding:WsdlOperationRef>
    </grounding:wsdlOperation>
    <grounding:wsdlInputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1:8000/wsdl/CityHotel#get_HOTELRequest</grounding:wsdlInputMessage>
    <grounding:wsdlOutputMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
    >http://127.0.0.1:8000/wsdl/CityHotel#get_HOTELResponse</grounding:wsdlOutputMessage>
    <grounding:wsdlInput>
      <grounding:WsdlInputMessageMap>
        <grounding:owlsParameter rdf:resource="#_CITY"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1:8000/wsdl/CityHotel#_CITY</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlInputMessageMap>
    </grounding:wsdlInput>
    <grounding:wsdlOutput>
      <grounding:WsdlOutputMessageMap>
        <grounding:owlsParameter rdf:resource="#_HOTEL"/>
        <grounding:wsdlMessagePart rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI"
        >http://127.0.0.1:8000/wsdl/CityHotel#_HOTEL</grounding:wsdlMessagePart>
        <grounding:xsltTransformationString>None (XSL)</grounding:xsltTransformationString>
      </grounding:WsdlOutputMessageMap>
    </grounding:wsdlOutput>
  </grounding:WsdlAtomicProcessGrounding>

</rdf:RDF>
